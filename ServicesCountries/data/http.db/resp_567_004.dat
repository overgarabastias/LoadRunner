@font-face
{
	font-family: 'MetricRegular';
	font-weight: normal;
	font-style: normal;
	src: url('./_HP_fonts/Regular/MetricWeb-Regular.eot'); /* IE9 Compat Modes */
	src: url('./_HP_fonts/Regular/MetricWeb-Regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
   	url('./_HP_fonts/Regular/MetricWeb-Regular.woff') format('woff'),
	url('./_HP_fonts/Regular/MetricApp-Regular.ttf') format('truetype'),
	url('./_HP_fonts/Regular/Metric-Regular.otf') format('opentype');
} 

